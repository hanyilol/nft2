{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "DataCollector": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "twitterHandle": {
          "type": "string"
        },
        "web3Wallet": {
          "type": "string"
        },
        "gigaPoints": {
          "type": "integer"
        }
      },
      "required": ["id", "name"]
    }
  }
}